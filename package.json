{
  "name": "vscode-changelog-generator",
  "displayName": "changelog-generator",
  "description": "An extension to generate changelog.",
  "version": "0.4.1",
  "publisher": "axetroy",
  "engines": {
    "vscode": "^1.21.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/axetroy/vscode-changelog-generator.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/axetroy/vscode-changelog-generator/issues"
  },
  "homepage": "https://github.com/axetroy/vscode-changelog-generator#readme",
  "keywords": [
    "changelog"
  ],
  "categories": [
    "Other"
  ],
  "icon": "resources/icon.png",
  "main": "./out/index",
  "activationEvents": [
    "onCommand:changelog.generate",
    "onCommand:changelog.generateFromLastVersion",
    "onCommand:changelog.generateFromLastTwoVersion",
    "onCommand:changelog.generateFromLastNVersion",
    "onCommand:changelog.generateAll"
  ],
  "contributes": {
    "commands": [
      {
        "command": "changelog.generate",
        "title": "%cmd.generate.title%",
        "category": "CHANGELOG"
      },
      {
        "command": "changelog.generateFromLastVersion",
        "title": "%cmd.generateFromLastVersion.title%",
        "category": "CHANGELOG"
      },
      {
        "command": "changelog.generateFromLastTwoVersion",
        "title": "%cmd.generateFromLastTwoVersion.title%",
        "category": "CHANGELOG"
      },
      {
        "command": "changelog.generateFromLastNVersion",
        "title": "%cmd.generateFromLastNVersion.title%",
        "category": "CHANGELOG"
      },
      {
        "command": "changelog.generateAll",
        "title": "%cmd.generateAll.title%",
        "category": "CHANGELOG"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "when": "resourceFilename =~ /^changelog(.md)?$/i",
          "command": "changelog.generate",
          "group": "changelog"
        },
        {
          "when": "resourceFilename =~ /^changelog(.md)?$/i",
          "command": "changelog.generateFromLastVersion",
          "group": "changelog"
        },
        {
          "when": "resourceFilename =~ /^changelog(.md)?$/i",
          "command": "changelog.generateFromLastTwoVersion",
          "group": "changelog"
        },
        {
          "when": "resourceFilename =~ /^changelog(.md)?$/i",
          "command": "changelog.generateFromLastNVersion",
          "group": "changelog"
        },
        {
          "when": "resourceFilename =~ /^changelog(.md)?$/i",
          "command": "changelog.generateAll",
          "group": "changelog"
        }
      ],
      "commandPalette": [
        {
          "command": "changelog.generate"
        },
        {
          "command": "changelog.generateFromLastVersion"
        },
        {
          "command": "changelog.generateFromLastTwoVersion"
        },
        {
          "command": "changelog.generateFromLastNVersion"
        },
        {
          "command": "changelog.generateAll"
        }
      ]
    },
    "configuration": {
      "title": "CHANGELOG",
      "properties": {
        "changelog.preset": {
          "type": "string",
          "default": "angular",
          "enum": [
            "angular",
            "atom",
            "codemirror",
            "ember",
            "eslint",
            "express",
            "jquery",
            "jscs",
            "jshint"
          ],
          "markdownDescription": "%ext.config.preset%"
        },
        "changelog.type": {
          "type": "string",
          "default": "new document",
          "enum": [
            "new document",
            "overwrite CHANGELOG.md",
            "append to CHANGELOG.md"
          ],
          "markdownEnumDescriptions": [
            "%ext.config.type-1%",
            "%ext.config.type-2%",
            "%ext.config.type-3%"
          ],
          "markdownDescription": "%ext.config.type%"
        },
        "changelog.release-count": {
          "type": "number",
          "default": 0,
          "markdownDescription": "%ext.config.release-count%"
        },
        "changelog.changelogFileName": {
          "type": "string",
          "default": "CHANGELOG.md",
          "markdownDescription": "%ext.config.changelogFileName%"
        },
        "changelog.outputUnreleased": {
          "type": "boolean",
          "default": false,
          "markdownDescription": "%ext.config.outputUnreleased%"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "npm run lint && npm run clean && npx @zeit/ncc build src/index.ts -o ./dist -e vscode -m && npm run clean && npx move-cli ./dist ./out",
    "clean": "npx rimraf ./out",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "lint": "tslint -p ./ -c tslint.json",
    "test": "npm run compile && node ./node_modules/vscode/bin/test",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "publish": "npx vsce publish"
  },
  "devDependencies": {
    "@types/execa": "0.9.0",
    "@types/mocha": "5.2.7",
    "@types/node": "11.15.4",
    "tslint": "6.0.0",
    "tslint-config-prettier": "1.18.0",
    "typescript": "3.7.5",
    "vscode": "1.1.36"
  },
  "dependencies": {
    "conventional-changelog-cli": "^2.0.12",
    "execa": "^4.0.0",
    "vscode-nls-i18n": "^0.1.1"
  }
}
